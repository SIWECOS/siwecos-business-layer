{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-snappy": "^0.4.4",
        "cviebrock/laravel-elasticsearch": "^3.6",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "^4.0",
        "gladcodes/keygen": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "h4cc/wkhtmltopdf-amd64": "0.12.4",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "~1.0",
        "league/flysystem-sftp": "^1.0",
        "staudenmeir/belongs-to-through": "^2.4",
        "stevegrunwell/lost-in-translation": "dev-fix/monolog-use-files",
        "voku/simple_html_dom": "^4.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "*",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mathiasgrimm/laravel-dot-env-gen": "dev-master",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "^7.0",
        "stechstudio/laravel-php-cs-fixer": "^1.1",
        "timacdonald/log-fake": "^1.2"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
